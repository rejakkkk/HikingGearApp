<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hikingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SeriveFoodApp">
        <activity
            android:name="com.example.hikingapp.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hikingapp.HomeActivity"
            android:exported="true"/>
        <activity
            android:name="com.example.hikingapp.Bar.CartListActivity"
            android:exported="true"/>
        <activity
            android:name="com.example.hikingapp.Bar.ArticleActivity"
            android:exported="true"/>
        <activity
            android:name="com.example.hikingapp.Bar.ReviewActivity"
            android:exported="true"/>
        <activity
            android:name="com.example.hikingapp.Bar.ProfileActivity"
            android:exported="true"/>
        <activity
            android:name="com.example.hikingapp.LoginActivity"
            android:exported="true"/>
        <activity
            android:name="com.example.hikingapp.RegisterActivity"
            android:exported="true"/>
    </application>

</manifest>